<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.sist.exam_hw">

	<!-- 제조국에 따라 제조사들을 조회하기 위한 Query문 -->
	<select id="selectMakers" resultType="String" parameterType="string">
		select maker
		from car_country
		where country = #{country}
	</select>

</mapper>